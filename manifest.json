{
	"manifest_version": 2,
	"name": "Youtube Layout Fixer",
	"description": "Fixes the youtube layout by adding: disable_polymer=true to all youtube links",
	"version": "1.0",
	"icons": {"128": "icon128.png"},
	"browser_action": { "default_icon": "icon128.png" },
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"http://*/*",
		"https://*/*"
	],
	
	"background": {
		"scripts": ["background.js"]
	}
}